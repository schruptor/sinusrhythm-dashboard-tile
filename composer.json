{
    "name": "schruptor/laravel-dashboard-sinusthythm-tile",
    "description": "A tile for Laravel Dashboard that displays the time and the weather",
    "keywords": [
        "spatie",
        "laravel-dashboard-time-weather-tile"
    ],
    "homepage": "https://github.com/schruptor/laravel-dashboard-time-weather-tile",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Fl√ºgge ",
            "email": "patrick@schruptor.me",
            "homepage": "https://schruptor.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-dashboard": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4"
    },
    "autoload": {
        "psr-4": {
            "Schruptor\\Sinusrhythm\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Schruptor\\Sinusrhythm\\Tests\\": "tests"
        }
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Schruptor\\Sinusrhythm\\SinusrhythmTileServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
